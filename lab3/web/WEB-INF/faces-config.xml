<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/Wstep/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{Losowanie.wyslij}</from-action>
            <from-outcome>OK</from-outcome>
            <to-view-id>/Wstep/wygrana.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Losowanie.wyslij}</from-action>
            <from-outcome>NOT_OK</from-outcome>
            <to-view-id>/Wstep/przegrana.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/zad1/zgadywanie.xhtml.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{Zgaduj.wyslij}</from-action>
            <from-outcome>1</from-outcome>
            <to-view-id>/zad1/1.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Zgaduj.wyslij}</from-action>
            <from-outcome>2</from-outcome>
            <to-view-id>/zad1/2.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Zgaduj.wyslij}</from-action>
            <from-outcome>3</from-outcome>
            <to-view-id>/zad1/3.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Zgaduj.wyslij}</from-action>
            <from-outcome>4</from-outcome>
            <to-view-id>/zad1/4.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Zgaduj.wyslij}</from-action>
            <from-outcome>5</from-outcome>
            <to-view-id>/zad1/4.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Zgaduj.wyslij}</from-action>
            <from-outcome>trafiony</from-outcome>
            <to-view-id>/zad1/trafiony.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{Zgaduj.losuj}</from-action>
            <from-outcome>Zgadywanie</from-outcome>
            <to-view-id>/zad1/zgadywanie.xhtml.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/zad5/Survey.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{Survey.proceed}</from-action>
            <from-outcome>summarySurvey</from-outcome>
            <to-view-id>/zad5/summarySurvey.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>